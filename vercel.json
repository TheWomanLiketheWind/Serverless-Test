{
  "version": 2,
  "routes": [
    {
      "src": "/api/server/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/server/posts/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/server_rewrite/(.*)",
      "dest": "/api/server_rewrite.js"
    },
    {
      "src": "/api/server_rewrite/posts/(.*)",
      "dest": "/api/server_rewrite.js"
    }
  ]
}